---
export interface CustomProps {
  title: string
}

const { title } = Astro.props as CustomProps
---

<!doctype html>
<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>{title}</title>
  </head>

  <body>
    <slot />
  </body>
</html>
